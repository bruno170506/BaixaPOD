<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="br.com.appcoral"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="14"
        android:targetSdkVersion="19"
        tools:ignore="OldTargetApi" />

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION" />
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
	<uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />

        <application
            android:allowBackup="true"
            android:icon="@drawable/sonatacoral"
            android:label="@string/app_name"
            android:theme="@style/AppTheme" >
            <activity android:name="br.com.appcoral.activitys.ListagemCoralistaActivity" >
                <intent-filter>
                    <action android:name="android.intent.action.MAIN" />

                    <category android:name="android.intent.category.LAUNCHER" />
                </intent-filter>
            </activity>
            <activity android:name="br.com.appcoral.activitys.FormularioCoralistaActivity" />
            <activity android:name="br.com.appcoral.activitys.FormularioCobrancaMensalidadeActivity" />
            <activity android:name="br.com.appcoral.activitys.FormularioFluxoCaixaActivity" />
            <activity android:name="br.com.appcoral.activitys.ListagemMensalidadeActivity" />
            <activity android:name="br.com.appcoral.activitys.ListagemFluxoCaixaActivity" />
            <activity android:name="br.com.appcoral.activitys.MainActivity" />
            <activity android:name="br.com.appcoral.activitys.DefineValorMensalidadeActivity" />
            
        </application>

</manifest>